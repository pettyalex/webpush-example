:8080 {
  gzip

  root push-client  

  rewrite /static {
    r (.*)
    to /admin/static{1}
  }

  basicauth /admin user password
  proxy /admin localhost:8082 {
    without /admin
    }

  basicauth /api user password
  proxy /api localhost:8099 {
    without /api
  }
}

:8082 {
  root push-admin-page/build
}

